# Dependencies
DEPS_DIR = .deps
MAKE_DEPEND_C   = @MAKE_DEPEND_C@ $(DEFS) $(INCLUDES) $(CPPFLAGS)

dependencies: $(DEPS_DIR)/timestamp
$(DEPS_DIR)/trmem.d: $(srcdir)/trmem.c
	$(MAKE_DEPEND_C)  $(srcdir)/trmem.c >$(DEPS_DIR)/trmem.d
$(DEPS_DIR)/handlemem.d: $(srcdir)/handlemem.c
	$(MAKE_DEPEND_C)  $(srcdir)/handlemem.c >$(DEPS_DIR)/handlemem.d
$(DEPS_DIR)/safestr.d: $(srcdir)/safestr.c
	$(MAKE_DEPEND_C)  $(srcdir)/safestr.c >$(DEPS_DIR)/safestr.d
$(DEPS_DIR)/argstr.d: $(srcdir)/argstr.c
	$(MAKE_DEPEND_C)  $(srcdir)/argstr.c >$(DEPS_DIR)/argstr.d
$(DEPS_DIR)/timestamp: trmem.c handlemem.c safestr.c argstr.c Makefile
	rm -f $(DEPS_DIR)/newalldeps
	$(MAKE_DEPEND_C)  $(srcdir)/trmem.c >$(DEPS_DIR)/trmem.d
	echo "include $(DEPS_DIR)/trmem.d" >>$(DEPS_DIR)/newalldeps
	$(MAKE_DEPEND_C)  $(srcdir)/handlemem.c >$(DEPS_DIR)/handlemem.d
	echo "include $(DEPS_DIR)/handlemem.d" >>$(DEPS_DIR)/newalldeps
	$(MAKE_DEPEND_C)  $(srcdir)/safestr.c >$(DEPS_DIR)/safestr.d
	echo "include $(DEPS_DIR)/safestr.d" >>$(DEPS_DIR)/newalldeps
	$(MAKE_DEPEND_C)  $(srcdir)/argstr.c >$(DEPS_DIR)/argstr.d
	echo "include $(DEPS_DIR)/argstr.d" >>$(DEPS_DIR)/newalldeps
	if [ -s $(DEPS_DIR)/newalldeps ] ; then mv -f $(DEPS_DIR)/newalldeps $(DEPS_DIR)/alldeps ; fi
	date >$(DEPS_DIR)/timestamp
include $(DEPS_DIR)/alldeps
